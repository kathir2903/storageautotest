[randomrw-md0]
blocksize=4k,4k
continue_on_error=none
direct=1
filename=/dev/md0
fsync=0
iodepth=64
ioengine=libaio
name=randomrw-md0
readwrite=randrw
runtime=300
rwmixread=80
thread=1
time_based

[randomrw-md1]
