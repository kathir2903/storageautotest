[randomrwmicron-rssda]
blocksize=4k,4k
continue_on_error=none
direct=1
filename=/dev/rssda
fsync=0
iodepth=64
ioengine=libaio
name=randomrwmicron-rssda
readwrite=randrw
runtime=300
rwmixread=80
thread=1
time_based

[randomrwmicron-rssdb]
