[randomrw-nvme0n1]
blocksize=4k,4k
continue_on_error=none
direct=1
filename=/dev/nvme0n1
fsync=0
iodepth=64
ioengine=libaio
name=randomrw-nvme0n1
readwrite=randrw
runtime=300
rwmixread=80
thread=1
time_based

